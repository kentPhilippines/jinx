            </if>
            <if test="retain2 != null" >
                retain2 = #{retain2,jdbcType=CHAR},
            </if>
            <if test="witType != null" >
                witType = #{witType},
            </if>
            <if test="appOrderId != null" >
                appOrderId = #{appOrderId},
            </if>
        </set>
        where id = #{id,jdbcType=INTEGER}
