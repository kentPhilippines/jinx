    <sql id="selectAlipayWithdrawEntityVo">
        select id, orderId, userId, withdrawType, bankNo, accname,macthTime,sgin,
         orderStatus, bankName, amount, fee, actualAmount,amount1, fee1, actualAmount1, mobile,watingTime,
         notify, createTime, submitTime, status, retain1, retain2, apply, comment, approval,witChannel,witType,
